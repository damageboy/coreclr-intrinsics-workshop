root = true 

# All files 
[*] 
indent_style = space
indent_size = 4
end_of_line = lf
charset = utf-8
trim_trailing_whitespace = true
insert_final_newline = true

[*.cs]
csharp_new_line_before_open_brace  = types, methods
csharp_style_var_elsewhere = true:silent
csharp_style_var_for_built_in_types = true:silent
